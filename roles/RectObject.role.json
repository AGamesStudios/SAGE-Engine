{
  "name": "RectObject",
  "description": "Простой объект-прямоугольник, отрисовываемый через Graphic.draw_rect",
  "fields": {
    "x": { "type": "float", "default": 0 },
    "y": { "type": "float", "default": 0 },
    "width": { "type": "float", "default": 10 },
    "height": { "type": "float", "default": 10 },
    "color": { "type": "color", "default": "#ffffff" }
  },
  "draw": [
    "Graphic.draw_rect(x, y, width, height, color)"
  ]
}
